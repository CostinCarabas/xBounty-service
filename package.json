{
  "name": "mx-nestjs-monorepo",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:offline-jobs": "nest build offline-jobs",
    "build:db": "nest build db",
    "build:all": "nest build && nest build offline-jobs",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "node dist/apps/api/src/main",
    "start:dev": "yarn copy-config && nest start --watch",
    "start:offline-jobs": "node dist/apps/offline-jobs/src/main",
    "start:offline-jobs:dev": "yarn copy-config:offline-jobs && nest start offline-jobs --watch",
    "copy-config": "run-script-os",
    "copy-config:nix": "mkdir -p dist/apps/api/src/config && cp ./apps/api/src/config/config.yaml ./dist/apps/api/src/config/config.yaml && cp ./apps/api/src/config/schema.yaml ./dist/apps/api/src/config/schema.yaml",
    "copy-config:windows": "(IF NOT EXIST dist\\apps\\api\\src\\config MKDIR dist\\apps\\api\\src\\config) && copy apps\\api\\src\\config\\config.yaml dist\\apps\\api\\src\\config\\config.yaml && copy apps\\api\\src\\config\\schema.yaml dist\\apps\\api\\src\\config\\schema.yaml",
    "copy-config:offline-jobs": "run-script-os",
    "copy-config:offline-jobs:nix": "mkdir -p dist/apps/offline-jobs/src/config && cp ./apps/offline-jobs/src/config/config.yaml ./dist/apps/offline-jobs/src/config/config.yaml && cp ./apps/offline-jobs/src/config/schema.yaml ./dist/apps/offline-jobs/src/config/schema.yaml",
    "copy-config:offline-jobs:windows": "(IF NOT EXIST dist\\apps\\offline-jobs\\src\\config MKDIR dist\\apps\\offline-jobs\\src\\config) && copy apps\\offline-jobs\\src\\config\\config.yaml dist\\apps\\offline-jobs\\src\\config\\config.yaml && copy apps\\offline-jobs\\src\\config\\schema.yaml dist\\apps\\offline-jobs\\src\\config\\schema.yaml",
    "lint": "eslint --max-warnings=0",
    "lint:fix": "eslint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "typeormcli:config": "node --require ts-node/register ./node_modules/typeorm/cli.js -d typeorm.config.ts",
    "typeormcli": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "yarn build:db && ts-node ./scripts/migrations/generate-migration.ts",
    "migration:run": "yarn build:db && yarn typeormcli:config migration:run",
    "migration:create": "yarn build:db && ts-node ./scripts/migrations/create-migration.ts"
  },
  "dependencies": {
    "@multiversx/sdk-core": "^13.15.0",
    "@multiversx/sdk-event-processor": "^1.0.2",
    "@multiversx/sdk-nestjs-auth": "^4.1.0",
    "@multiversx/sdk-nestjs-cache": "^4.1.0",
    "@multiversx/sdk-nestjs-common": "^4.1.0",
    "@multiversx/sdk-nestjs-monitoring": "^4.1.0",
    "@multiversx/sdk-nestjs-rabbitmq": "^4.1.0",
    "@multiversx/sdk-nestjs-redis": "^4.1.0",
    "@multiversx/sdk-network-providers": "^2.9.3",
    "@multiversx/sdk-transaction-processor": "^0.1.35",
    "@nestjs/common": "^10.4.15",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.15",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^8.1.0",
    "@nestjs/typeorm": "^10.0.2",
    "@octokit/auth-app": "^7.1.3",
    "axios": "^1.7.7",
    "bech32": "^2.0.0",
    "cron": "3.2.1",
    "ioredis": "^5.4.1",
    "js-yaml": "^4.1.0",
    "mysql2": "^3.11.3",
    "nest-winston": "^1.9.7",
    "nestjs-octokit": "^0.2.0",
    "octokit": "^4.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "qrcode": "^1.5.4",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.1",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.13.0",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.6",
    "@stylistic/eslint-plugin-js": "^2.9.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.7.9",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.11.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "run-script-os": "^1.1.6",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "resolutions": {
    "micromatch": "4.0.8"
  }
}
